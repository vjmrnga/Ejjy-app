{
	"name": "ejjy-inventory-app",
	"version": "0.1.481",
	"private": true,
	"homepage": "./",
	"main": "./public/electron.js",
	"scripts": {
		"lint": "cd src && eslint . --ext .tsx --ext .ts",
		"lint:fix": "cd src && eslint . --ext .tsx --ext .ts --fix",
		"prettify": "prettier --write ./src",
		"deploy": "npm version patch --no-git-tag-version && python deploy.py",
		"start:online": "env-cmd -f .env.online react-scripts start",
		"start:local": "env-cmd -f .env.local react-scripts start",
		"start:electron": "concurrently \"PORT=3010 env-cmd -f .env.local react-scripts start\" \"wait-on http://localhost:3010 && electron .\"",
		"server:bm": "cd ./api && poetry run python manage.py runserver 8001",
		"server:bm:branch": "concurrently \"npm run server:bm\" \"cd ../ejjy-app-cashiering/api && poetry run python manage.py runserver 8002\"",
		"start:bm": "concurrently \"npm run server:bm:branch\" \"npm run start:local\"",
		"server:om": "cd ./ejjy-api && .\\env\\Scripts\\activate && poetry run python manage.py runserver 8002",
		"server:om:branch": "concurrently \"npm run server:bm\" \"npm run server:om\"",
		"start:om": "concurrently \"npm run server:om:branch\" \"npm run start:local\"",
		"server:all": "concurrently \"npm run server:bm:branch\" \"npm run server:om\"",
		"electron": "wait-on http://localhost:3010 && electron .",
		"postinstall": "electron-builder install-app-deps",
		"build:online": "env-cmd -f .env.online react-scripts build",
		"build:local": "env-cmd -f .env.local react-scripts build",
		"build:electron": "electron-builder -w --x64 -p always --config electron-builder.json",
		"build:electron:headoffice": "electron-builder -w --x64 -p always --config electron-builder-headoffice.json",
		"publish:backoffice": "npm version patch --no-git-tag-version && npm run build:local && npm run build:electron",
		"publish:headoffice": "npm version patch --no-git-tag-version && npm run build:local && npm run build:electron:headoffice",
		"publish": "npm version patch --no-git-tag-version && npm run build:local && npm run build:electron && npm run build:electron:headoffice",
		"tsc": "tsc --noEmit --skipLibCheck"
	},
	"dependencies": {
		"@ant-design/colors": "7.0.0",
		"@ant-design/icons": "4.2.2",
		"antd": "4.24.8",
		"antd-css-utilities": "1.0.3",
		"array-move": "4.0.0",
		"axios": "^0.20.0",
		"classnames": "2.2.6",
		"connected-react-router": "6.8.0",
		"dayjs": "1.10.4",
		"ejjy-global": "github:EJJY-Devs/ejjy-global",
		"electron-is-dev": "2.0.0",
		"electron-log": "4.4.1",
		"electron-store": "8.1.0",
		"electron-updater": "4.3.9",
		"focus-formik-error": "1.1.0",
		"formik": "2.2.6",
		"history": "4.10.1",
		"i": "0.3.7",
		"jsbarcode": "3.11.5",
		"jspdf": "2.3.1",
		"lodash": "4.17.21",
		"moment": "^2.30.1",
		"npm": "8.13.2",
		"qrcode": "1.5.1",
		"query-string": "7.0.1",
		"qz-tray": "2.1.2",
		"rc-resize-observer": "0.2.3",
		"react": "^16.14.0",
		"react-barcode-reader": "0.0.2",
		"react-beforeunload": "2.4.0",
		"react-custom-scrollbars": "4.2.1",
		"react-dom": "16.13.1",
		"react-helmet": "6.1.0",
		"react-keyboard-event-handler": "1.5.4",
		"react-markdown": "8.0.3",
		"react-query": "3.34.15",
		"react-redux": "7.2.2",
		"react-router": "5.3.4",
		"react-router-dom": "5.3.4",
		"react-scripts": "4.0.3",
		"react-sortable-hoc": "2.0.0",
		"react-window": "1.8.5",
		"recharts": "2.1.16",
		"redux": "4.1.0",
		"redux-actions": "2.6.5",
		"redux-persist": "6.0.0",
		"redux-saga": "1.1.3",
		"remark-gfm": "3.0.1",
		"reselect": "4.0.0",
		"sweetalert": "2.1.2",
		"tree-kill": "1.2.2",
		"typescript": "4.1.6",
		"use-debounce": "7.0.1",
		"use-interval": "1.4.0",
		"yup": "0.29.3",
